pipeline {
    agent any

    stages {
        stage('run tests') {
            steps{
                build job: 'mpipeline', wait: false
                parameters: [
                    string(name: 'server', value: "server1"),
                    string(name: 'rest', value: "true"),
                    string(name: 'web', value: "false")
                ]
            }
        }
    }
}